
<head>
  <script type="text/javascript">
     var floorPlanId = $('h1').attr('data-id')
    $.get('/floor_plan/' + floorPlanId, function(response) {
      var div = $('#get_floorplan', $(response))
      $('div.second').html(response)
    })
   </script>
</head>
<div id="customer-floor-plan">
  <% if current_restaurant_profile %>
  <ul class="restaurant_nav">
    <li><%= link_to "View Outstanding Orders", carts_url %></li>
    <li><%= link_to "Back to Dashboard", restaurant_dashboard_url %></li>
  </ul>
  <% end %>
  <% if current_customer_profile %>
    <h1 id="hide-confirm" class="title">Check-In</h1>
    <h3 class="title">Click a green seat to check-in</h3>
  <% elsif current_restaurant_profile %>
    <h1 class="title" id="check-seats" data-id=<%= @floor_plan.id %>>Your Current Seating Plan</h1>
  <% end %>
  <div id="dialog" class="ui-helper-hidden" title="Seat Confirmation Required">Are you sure you want to check-in to this seat?</div>

  <div id="get_floorplan">

  </div>
</div>
  <% if current_customer_profile %>
  <nav class="customer_nav">
    <ul>
      <li class="left_option"><%= link_to "Edit Profile", edit_customer_profile_url(current_customer_profile) %></li>
      <li class="right_option"><%= link_to "Find Restaurant", customer_find_restaurant_profiles_url %></li>
    </ul>
  </nav>
<% end %>