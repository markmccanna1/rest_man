<div class="active-carts">
  <ul class="restaurant_nav">
    <li><%= link_to "View Outstanding Orders", carts_url %></li>
    <li><%= link_to "Back to Dashboard", restaurant_dashboard_url %></li>
  </ul>
  <h1 id="rest<%= current_restaurant_profile.id %>" class="title"><%= current_restaurant_profile.restaurant_name %> Open Carts</h1>
  <ul class="order_list">
    <% @confirmed_carts.each do |cart| %>
    <li class="order"><a href="carts/<%= cart.id %>" id="<%= cart.id %>">Cart #<%= cart.id %>  Table <%= @table.id %> Seat <%= @seat.id %></a></li>
    <% end %>
  </ul>
  <ul class="pending-carts">
  </ul>
</div>
