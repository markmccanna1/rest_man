<div>
  <h1 data-id=<%=current_restaurant_profile.id%> class='title' id="restaurant-dashboard-header">Welcome to the Dashboard for <%= current_restaurant_profile.restaurant_name %></h1>

  <ul class="restaurant_nav">
    <% if current_restaurant_profile.floor_plan %>
      <li><%= link_to "Current FloorPlan", floor_plan_url(@restaurant.floor_plan.id) %></li>
    <% end %>
    <li><%= link_to "Add New FloorPlan", new_floor_plan_url(@restaurant) %></li>
    <li><%= link_to "View Outstanding Orders", carts_url %></li>
    <li><%= link_to "Edit Profile", edit_restaurant_profile_url(@restaurant) %></li>
    <li class="create-menu-button">Create a New Menu</li>
     <% if flash[:notice] %>
    <br><br><div id="success-notice"><h2><%= flash[:notice] %></h2></div>
  <% end %>
  </ul>

 

  <div class="new-menu">
  </div>
</div>

